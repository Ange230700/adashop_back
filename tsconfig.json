{
  "compilerOptions": {
    "target": "ES2023",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "~/*": ["./*"]
    },
    "noImplicitAny": true
  }
}
